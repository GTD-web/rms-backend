{
    "$schema": "https://json.schemastore.org/nest-cli",
    "collection": "@nestjs/schematics",
    "sourceRoot": "apps/resource/src",
    "entryFile": "main",
    "compilerOptions": {
        "deleteOutDir": true,
        "webpack": true,
        "tsConfigPath": "apps/resource/tsconfig.app.json"
    },
    "monorepo": true,
    "root": "apps/resource",
    "projects": {
        "resource": {
            "type": "application",
            "root": "apps/resource/src",
            "entryFile": "main",
            "sourceRoot": "apps/resource/src",
            "compilerOptions": {
                "tsConfigPath": "apps/resource/tsconfig.app.json"
            }
        }
    }
}
