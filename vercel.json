{
    "version": 2,
    "builds": [
        {
            "src": "dist/apps/resource/main.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "dist/apps/resource/main.js",
            "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"]
        }
    ],
    "crons": [
        {
            "path": "/api/v1/reservations/cron-job/close",
            "schedule": "1 * * * *"
        },
        {
            "path": "/api/v1/reservations/cron-job/close",
            "schedule": "31 * * * *"
        }
    ]
}
